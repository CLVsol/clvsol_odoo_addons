<odoo>

    <record id="clv_processing_schedule_execute_view_form" model="ir.ui.view">
        <field name="name">clv.processing.schedule.execute.form</field>
        <field name="model">clv.processing.schedule.execute</field>
        <field name="arch" type="xml">
            <form>
                <div class="oe_right">
                </div>
                <label for="processing_schedule_ids" class="oe_edit_only"/>
                <field name="processing_schedule_ids"/>
                <group>
                </group>
                <footer>
                    <button 
                        type="object"
                        name="do_processing_schedule_execute"
                        string="Processing Schedule Execute"
                        class="oe_highlight"
                        />
                    <button special="cancel" string="Cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <!-- More button Action -->
    <act_window 
        id="clv_processing_schedule_execute_action"
        name="Processing Schedule Execute"
        src_model="clv.processing.schedule"
        res_model="clv.processing.schedule.execute"
        view_mode="form"
        key2="client_action_multi"
        target="new"
    />

</odoo>

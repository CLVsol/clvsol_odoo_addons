<odoo>
        
    <record id="clv_family_view_form_name" model="ir.ui.view">
        <field name="name">clv.family.form</field>
        <field name="model">clv.family</field>
        <field name="inherit_id" ref="clv_family_view_form"/>
        <field name="priority" eval="10" />
        <field name="arch" type="xml">
            <field name="type" position="before">
                <field name="suggested_name"/>
            </field>
            <group name="settings_left_3" position="inside">
                <field name="automatic_set_name"/>
            </group>
            <page name="settings" position="attributes">
                <attribute name="invisible">False</attribute>
            </page>
        </field>
    </record>

</odoo>
